{
  "name": "disaster-preparedness-dashboard",
  "version": "1.0.0",
  "description": "A disaster preparedness calculator that evaluates risks and calculates necessary supplies.",
  "main": "src/index.tsx",
  "scripts": {
    "start": "react-scripts start",
    "build:css": "tailwindcss -i ./src/styles/index.css -o ./src/styles/tailwind.css --minify",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -r https://x-access-token:${GITHUB_TOKEN}@github.com/${npm_package_repository_url}.git"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "recharts": "^2.1.9",
    "lucide-react": "^0.268.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^4.9.5",
    "react-scripts": "^5.0.1",
    "gh-pages": "^6.0.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0"
  },
  "homepage": "https://ToyMoy-Pattoiku.github.io/",
  "license": "MIT"
}